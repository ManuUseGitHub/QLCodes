{
  "name": "qlcodes",
  "version": "1.3.2",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs",
    "./codes": "./dist/qlCodes.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./shell/build.sh",
    "fetch": "node ./src/fetch.mjs | ./shell/display.sh",
    "fetch2": "node ./src/fetch.mjs",
    "refetch": "./shell/cleanUpCsvReferences.sh",
    "dev": "clear; cd ./shell; chmod +x ./*.sh; cd .. ; ./shell/settle.sh"
  },
  "author": "Jean Luc Emmanuel VERHANNEMAN",
  "license": "MIT",
  "description": "A small utility that unifies SQL error codes (SQLSTATE) from different DBMS into a single, normalized structure through a curated dataset constructed at build time.",
  "keywords": [
    "sqlstate",
    "rdbms",
    "sql",
    "error-codes",
    "database",
    "MySQL",
    "MariaDB",
    "PostgreSQL",
    "CockroachDB",
    "Microsoft SQL Server",
    "Oracle",
    "SQLite",
    "SAP HANA",
    "Google Spanner",
    "IBM"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuUseGitHub/QLCodes.git"
  },
  "devDependencies": {
    "@maze014/dom-fetch": "^1.2.1",
    "csv-parse": "^6.1.0",
    "onstdin": "^1.0.0",
    "reargv": "^1.0.2",
    "to-snake-case": "^1.0.0"
  }
}